{
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx"
  },
  "tasks": {
    "dev": "deno run -A npm:wrangler dev",
    "deploy": "deno run -A npm:wrangler deploy --minify",
    "cf-typegen": "deno run -A npm:wrangler types --env-interface CloudflareBindings",
    "test": "deno test src/lib/"
  },
  "imports": {
    "hono": "npm:hono@^4.12.0",
    "hono/": "npm:hono@^4.12.0/",
    "hono/jsx/jsx-runtime": "npm:hono@^4.12.0/jsx/jsx-runtime"
  }
}
